{
  "expo": {
    "name": "Yann Home",
    "slug": "yann-mobile",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/Logo.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "yann",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yanncare.mobile",
      "googleServicesFile": "./GoogleService-Info.plist",
      "buildNumber": "7",
      "associatedDomains": [
        "applinks:yann.app",
        "applinks:www.yann.app"
      ],
      "config": {
        "googleMapsApiKey": "AIzaSyDuWW76Kch_KG5n9vxNwSq3GfJCSSZuBOg"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Allow Yann to use your location for accurate service delivery.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Allow Yann to use your location for accurate service delivery.",
        "NSCameraUsageDescription": "Allow Yann to access your camera to take a profile picture.",
        "NSPhotoLibraryUsageDescription": "Allow Yann to access your photos to upload a profile picture.",
        "NSPhotoLibraryAddUsageDescription": "Allow Yann to save photos to your library.",
        "NSMicrophoneUsageDescription": "Allow Yann to access your microphone for video recording.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.yanncare.mobile",
      "googleServicesFile": "./google-services.json",
      "versionCode": 7,
      "userInterfaceStyle": "light",
      "adaptiveIcon": {
        "foregroundImage": "./assets/Logo.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO",
        "android.permission.VIBRATE",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.RECEIVE_BOOT_COMPLETED"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDuWW76Kch_KG5n9vxNwSq3GfJCSSZuBOg"
        }
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "yann.app",
              "pathPrefix": "/provider"
            },
            {
              "scheme": "https",
              "host": "www.yann.app",
              "pathPrefix": "/provider"
            },
            {
              "scheme": "https",
              "host": "yann.app",
              "pathPrefix": "/verification-success"
            },
            {
              "scheme": "https",
              "host": "www.yann.app",
              "pathPrefix": "/verification-success"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8abeb1ba-a4c4-4269-aca9-39974e9e40f4"
      }
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Yann to use your location for accurate service delivery."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Yann to access your photos to upload a profile picture.",
          "cameraPermission": "Allow Yann to access your camera to take a profile picture."
        }
      ],
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/Logo.jpg",
          "color": "#ffffff",
          "defaultChannel": "default",
          "sounds": [
            "./assets/sounds/booking_request.wav"
          ]
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/8abeb1ba-a4c4-4269-aca9-39974e9e40f4"
    }
  }
}